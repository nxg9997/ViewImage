{
    "name": "Google View Image",
    "description" : "Adds View Image button",
    "version": "0.1",
    "manifest_version": 2,
    "browser_action": {
      "default_popup": "index.html",
      "default_icon": "icon.png"
    },

    "content_scripts": [{
        "js": ["code.js"],
        "matches": ["*://www.google.com/*"]
    }],

    "permissions": ["tabs", "<all_urls>"]
  }